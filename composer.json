            "email": "mail@sudipto.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "sudiptochoudhury/php-api-client-forge": "0.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2.0",
        "mockery/mockery": "^1.1.0"
