    "keywords":["zoho", "subscriptions", "php", "client", "rest", "api"],
    "type": "library",
    "homepage": "https://github.com/sudiptochoudhury/php-zoho-subscriptions",
    "license": "MIT",
    "authors": [
        {
            "name": "Sudipto Choudhury",
